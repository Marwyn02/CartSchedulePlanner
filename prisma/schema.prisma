generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Appointment {
  id   Int    @id @default(autoincrement())
  date String
  place String
  time String?
  user User[] @relation("AppointmentToUser")
}

model User {
  id          Int           @id @default(autoincrement())
  name        String
  appointment Appointment[] @relation("AppointmentToUser")
}
